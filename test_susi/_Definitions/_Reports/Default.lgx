<?xml version="1.0" encoding="utf-8"?>
<Report
	ID="Default"
	>
	<Body>
		<Division
			HtmlDiv="True"
			ID="divtest"
			>
			<Label
				Caption="@Constant.schema_name~"
			/>
			<DataTable
				AjaxPaging="True"
				ID="divTable"
				Width="300"
				WidthScale="px"
				>
				<DataLayer
					ID="dlstatic"
					Type="Static"
					>
					<StaticDataRow
						key="text1"
						value="5"
					/>
					<StaticDataRow
						key="text4"
						value="4"
					/>
					<StaticDataRow
						key="text5"
						value="3"
					/>
					<StaticDataRow
						key="test10"
						value="50"
					/>
				</DataLayer>
				<DataTableColumn
					Class="test1"
					Header="Key"
					>
					<Label
						Caption="@Data.key~"
					/>
					<DataColumnSort
						DataColumn="key"
					/>
				</DataTableColumn>
				<DataTableColumn
					Header="Value"
					>
					<Label
						Caption="@Data.value~"
					/>
					<DataColumnSort
						DataColumn="value"
					/>
				</DataTableColumn>
				<DataTableColumn
					Class="ThemeAlignRight"
					Header="key"
					ID="colkey"
					>
					<Label
						Caption="@Data.key~"
						ID="lblkey"
					/>
					<DataColumnSort
						DataColumn="key"
					/>
				</DataTableColumn>
			</DataTable>
		</Division>
	</Body>
	<StyleSheet
		StyleSheet="test.css"
		Theme="Clarity"
	/>
	<ideTestParams/>
</Report>
